{
  "name": "chain-mock",
  "version": "0.1.0",
  "description": "Mock fluent/chainable APIs with full call tracking and cross-framework matcher support",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts"
    },
    "./jest": {
      "types": "./dist/jest.d.ts"
    },
    "./bun": {
      "types": "./dist/bun.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mock",
    "chainable",
    "fluent",
    "testing",
    "proxy",
    "jest",
    "vitest",
    "bun",
    "matchers"
  ],
  "scripts": {
    "lint": "prettier --check . && eslint --max-warnings 0 .",
    "lint:fix": "prettier --write . && eslint --max-warnings 0 --fix .",
    "test": "vitest run",
    "build": "rolldown -c"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "24.9.0",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint": "9.39.2",
    "eslint-config-loderunner": "3.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.5.0",
    "globals": "17.0.0",
    "prettier": "3.7.4",
    "rolldown": "1.0.0-rc.1",
    "rolldown-plugin-dts": "^0.21.6",
    "testcontainers": "^11.11.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "@types/bun": ">=1.0.0",
    "jest": ">=29.0.0",
    "vitest": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "@types/bun": {
      "optional": true
    }
  }
}
